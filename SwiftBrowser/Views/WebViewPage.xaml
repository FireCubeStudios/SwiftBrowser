                                                                <MenuFlyoutSeparator/>
                                                                <MenuFlyoutItem Text="Send feedback" Click="FeedbackLink_Click"/>
                                                                <MenuFlyoutItem Text="Rate" Click="Rate"/>
                                                                <MenuFlyoutSeparator/>
                                                                <MenuFlyoutItem Text="What's new" Click="WhatNew"/>
